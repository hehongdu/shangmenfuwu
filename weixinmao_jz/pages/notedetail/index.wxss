    background-color: rgba(51,51,51,.44);
    text-align: center;
    color: #fff;
    font-size: 26rpx;
}

.data .main .technician .item .left .state {
    float: left;
    width: 140rpx;
    height: 40rpx;
    line-height: 40rpx;
    background: #e7e5e5;
    border-radius: 20px;
    color: #666;
    font-size: 26rpx;
    color: #fff;
    text-align: center;
