    right: 0;
}

.data .main .technician .item .check.checked {
    float: left;
    width: 40rpx;
    height: 240rpx;
    background: url("https://oss.angemo.com/images/app/custom/product/checked.png") no-repeat 50%;
    background-size: 40rpx 40rpx;
    position: absolute;
    top: 0;
    right: 0;
}

.data .main .technician .item:nth-child(even) {
