.data {
    margin-top: 10rpx;
    background-color: #fff;
}

.data,.data .head {
    float: left;
    width: 100%;
}

.data .head {
    height: 80rpx;
    border-bottom: 2rpx solid #eee;
}

.data .head .item {
    float: left;
    width: 250rpx;
    height: 80rpx;
    line-height: 80rpx;
    color: #666;
    font-size: 24rpx;
    text-align: center;
}

.data .head .item.active {
    background-image:-webkit-linear-gradient(180deg, #D434EC, #F8606D);
    -webkit-background-clip:text;
    -webkit-text-fill-color:transparent;
    position: relative;
}

.data .head .item.active:after {
    float: left;
    width: 190rpx;
    height: 6rpx;
    background: linear-gradient(0deg, #D434EC, #F8606D);
    position: absolute;
    left: 30rpx;
    bottom: -1rpx;
    border-radius: 3rpx;
    content: "";
}

.data .main {
    padding: 0 30rpx;
    box-sizing: border-box;
}

.data .main,.data .main .technician {
    float: left;
    width: 100%;
}

.data .main .technician .item {
    float: left;
    width: 100%;
    overflow: hidden;
    position: relative;
    padding: 20rpx 0;
    border-bottom: 2rpx solid #eee;
}

.data .main .technician .item .left {
    float: left;
    width: 220rpx;
    position: relative;
}

.data .main .technician .item .left .photo {
    float: left;
    width: 100%;
    height: 220rpx;
    border-radius: 40rpx;
