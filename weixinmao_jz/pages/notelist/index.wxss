.type-container {
    width: 100%;
    height: 88rpx;
    background-color: #fff;
    overflow: hidden;
    border-bottom: 1rpx solid #F2f2f2;
    padding-bottom: 10rpx;
    margin-bottom: 18rpx;
}

.type-navbar {
    white-space: nowrap;
    height: 88rpx;
    background-color: #fff;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.type-box {
    width: 24.5%;
    box-sizing: content-box;
    font-size: 24rpx;
    line-height: 84rpx;
    text-align: center;
    display: inline-block;
    overflow: hidden;
}

.type-navbar-item {
    border-bottom: 4rpx solid #fff;
}

.type-item-on {
    border-bottom: 4rpx solid #e64340;
}

.container {
    background-color: #f5f5f5;
    min-height: 100%;
}

.container .orderlist {
    width: 750rpx;
    display: flex;
    flex-direction: column;
    background: #FFF;
    margin-top: 10rpx;
}

.container .orderlist .orderid {
    border-bottom: 1rpx solid #efefef;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    padding-left: 20rpx;
    font-size: .8rem;
}

.container .orderlist .content {
    font-size: .8rem;
    border-bottom: 1rpx solid #efefef;
}

.container .orderlist .content .item {
    padding-top: 16rpx;
    padding-bottom: 16rpx;
    padding-left: 20rpx;
    border-bottom: 1rpx dotted #efefef;
    display: flex;
    flex-direction: row;
}

.container .orderlist .content .payitem {
    width: 750rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    padding-bottom: 20rpx;
    padding-top: 20rpx;
}

.container .orderlist .content .payitem view {
    width: 180rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    margin-right: 20rpx;
    border-radius: 5rpx;
    background: #3274e5;
    font-size: .6rem;
    color: #FFF;
}

.fuwulist {
  background:#fff;
  height:100%;
  width:95%;
  margin:0 auto 20rpx;
  min-height:200rpx;
  border-radius:10rpx;
}

.cu-load.over::after {
  content: '我是有底线的';
}

.list {
  height:100%;
  margin-bottom:20rpx;
}

.list .block {
  display: flex;
  justify-content: space-between;
  width:98%;
  margin: 0rpx auto;
  border-bottom:1px solid #f4f4f4;
  height:250rpx;
  padding:20rpx 0;
  align-items: center;
}

.list .block:last-child {
  border-bottom:0;
}

.list .block .left {
  width: calc(35% - 20rpx);
  margin-right:20rpx;
  height: 100%;
  padding:10rpx
}

.list .block .left image {
  height: 100%;
}

.list .block .right {
  width: 65%;
  height: 100%;
  display:flex;
  justify-content: space-between;
  flex-direction: column;
  flex:1;
  padding:5rpx 0
}

.list .block .right .des {
  font-size:20rpx;
}

.list .block .right .title {
  font-size:30rpx;
  color:#333333
}

.list .block .right .time{
  font-size:25rpx;
  color:#333333;
  display: flex;
  justify-content: flex-start;
  align-items:center;
}

.list .block .right .btn {
  height: 50rpx;
  font-size:20rpx;
  background: linear-gradient(0deg, #D434EC, #F8606D);
  border-radius: 10rpx;
  outline:none;
  color:#fff
}

.list .block .right .amount {
  font-size:24rpx;
  color:#333333;
  display: flex;
  align-items:center;
  height: 30rpx;
}


