.container {
    background-color: #f5f5f5;
    min-height: 100%;
}

.container .header {
    width: 750rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    background: #42BE9C;
    padding: 0 30rpx;
    height: 310rpx;
    border-bottom-right-radius: 20%;
    border-bottom-left-radius: 20%;
}

.container .header .pic {
    width: 380rpx;
    /* height: 250rpx; */
    align-self: center;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.container .header .pic image {
    width: 120rpx;
    height: 120rpx;
    border: 7rpx solid #FFF;
    border-radius: 120rpx;
}

.container .header .pic view {
    height: 80rpx;
    line-height: 80rpx;
    width: 380rpx;
    text-align: center;
    color: #FFF;
    font-size: .8rem;
}

.container .order {
    width: 750rpx;
    display: flex;
    flex-direction: column;
}

.container .order .title {
    font-size: 1rem;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    background: #FFF;
    margin-top: 20rpx;
    padding-left: 20rpx;
    border-bottom: 1rpx solid #efefef;
}

.container .order .content {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    width: 750rpx;
    background: #FFF;
    padding-bottom: 20rpx;
    padding-top: 20rpx;
}

.container .order .content .item {
    width: 250rpx;
    text-align: center;
    font-size: .8rem;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.container .order .content .item image {
    width: 100rpx;
    height: 100rpx;
}

.container .list {
    width: 750rpx;
    margin-top: 20rpx;
    background: #FFF;
}

.container .list .item {
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    font-size: .9rem;
    border-bottom: 1rpx solid #efefef;
    padding-left: 20rpx;
    color: #747474;
}

.container .list .item text {
    margin-right: 10rpx;
}

button::after {
    border: none;
}

.weixinmao-msg {
    position: absolute;
    top: 0%;
    left: 0%;
    width: 100%;
    height: 100%;
    background-color: black;
    z-index: 2001;
    background: rgba(0,0,0,0.7);
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.weixinmao-content {
    width: 600rpx;
    height: 450rpx;
    z-index: 4001;
    background: #FFF;
    align-self: center;
    border-radius: 8rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.weixinmao-content .title {
    border-bottom: 1rpx solid #e5e5e5;
    width: 100%;
    text-align: center;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    font-size: .8rem;
    background: #3274e5;
    color: #FFF;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.weixinmao-content .title view:nth-child(1) {
    width: 30%;
}

.weixinmao-content .title view:nth-child(2) {
    width: 40%;
}

.weixinmao-content .title view:nth-child(3) {
    width: 30%;
}

.weixinmao-content .content {
    display: flex;
    flex-direction: column;
    width: 100%;
    justify-content: center;
    margin-top: 20rpx;
}

.weixinmao-content .content .item {
    font-size: .8rem;
    width: 100%;
    align-self: center;
    margin-bottom: 10rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.inputlist {
    border: 1rpx solid #efefef;
    width: 80%;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 4rpx;
    align-self: center;
}

